void main () {
  //create person class
  Tao t1 = Tao("Pearl Joy Felia","Female",21);
  t1.showData();
}

class Tao{
  String? name, sex;
  int? age;
  
  //create constructors
  Tao(String name, sex, int age) {
    this.name = name;
    this.sex = sex;
    this.age = age;
  }
  
  //create methods, basically a function
  void showData () {
    print('Name = $name');
    print('Sex = $sex');
    print('Age = $age');
  }
}
